{
  "name": "electron-infra",
  "description": "Electron Infrastructure",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "author": "",
  "main": "build/main.js",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "package": "sh ./package.sh",
    "clean": "sh ./clean.sh",
    "lint:web": "eslint --fix-dry-run ./src/10-renderer --ext .tsx",
    "lint:app": "eslint --fix-dry-run ./src/20-main --ext .ts",
    "compile:web": "sh compile.sh web",
    "compile:app": "sh compile.sh app",
    "preserve:web": "yarn run lint:web",
    "serve:web": "react-app-rewired start",
    "preserve:app": "yarn run compile:app",
    "serve:app": "electron -r dotenv-flow/config ."
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@mui/x-data-grid": "^6.0.2",
    "@react-hook/size": "^2.1.2",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^0.27.2",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^4.9.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-window": "^1.8.8",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "sqlite3": "^5.1.6",
    "web-vitals": "^2.1.4",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.2.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/parser": "^5.56.0",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.6",
    "react-app-alias": "^2.2.2",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "tsc-alias": "^1.8.4",
    "typescript": "^5.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
